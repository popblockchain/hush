<!-- Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    -->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="12.5%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <!-- Row>
        <Key android:codes="301,331" android:keyLabel="ㅂ" android:keyEdgeFlags="left"/>
        <Key android:codes="302,332" android:keyLabel="ㅈ"/>
        <Key android:codes="303,333" android:keyLabel="ㄷ"/>
        <Key android:codes="304,334" android:keyLabel="ㄱ"/>
        <Key android:codes="305,335" android:keyLabel="ㅅ"/>
        <Key android:codes="316,306" android:keyLabel="ㅗ"/>
        <Key android:codes="309,336" android:keyLabel="ㅐ"/>
        <Key android:codes="310,337" android:keyLabel="ㅔ" android:keyEdgeFlags="right"/>
    </Row-->
    <Row>
        <Key android:codes="301" android:keyLabel="ㅂ" android:keyEdgeFlags="left"/>
        <Key android:codes="302" android:keyLabel="ㅈ"/>
        <Key android:codes="303" android:keyLabel="ㄷ"/>
        <Key android:codes="304" android:keyLabel="ㄱ"/>
        <Key android:codes="305" android:keyLabel="ㅅ"/>
        <Key android:codes="316" android:keyLabel="ㅗ"/>
        <Key android:codes="309" android:keyLabel="ㅐ"/>
        <Key android:codes="310" android:keyLabel="ㅔ" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="311" android:keyLabel="ㅁ" android:keyEdgeFlags="left"/>
        <Key android:codes="312" android:keyLabel="ㄴ"/>
        <Key android:codes="313" android:keyLabel="ㅇ"/>
        <Key android:codes="314" android:keyLabel="ㄹ"/>
        <Key android:codes="315" android:keyLabel="ㅎ"/>
        <!-- Key android:codes="317,307" android:keyLabel="ㅓ"/>
        <Key android:codes="318,308" android:keyLabel="ㅏ"/-->
        <Key android:codes="317" android:keyLabel="ㅓ"/>
        <Key android:codes="318" android:keyLabel="ㅏ"/>
        <Key android:codes="201" android:keyLabel="ㅣ" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-7" android:keyIcon="@drawable/sym_keyboard_numspec" 
                android:keyWidth="12.5%p" android:keyEdgeFlags="left"/>
        <Key android:codes="320" android:keyLabel="ㅋ"/>
        <Key android:codes="321" android:keyLabel="ㅌ"/>
        <Key android:codes="322" android:keyLabel="ㅊ"/>
        <Key android:codes="323" android:keyLabel="ㅍ"/>
        <Key android:codes="325,324" android:keyLabel="ㅜ"/>
        <Key android:codes="203" android:keyLabel="ㅡ"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="12.5%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key  android:codes="-8" android:keyIcon="@drawable/sym_keyboard_hanhan" android:keyWidth="12.5%p" android:keyEdgeFlags="left" />
        <Key android:codes="-2" android:keyIcon="@drawable/sym_keyboard_haneng" android:keyWidth="12.5%p" />
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="29%p" android:isRepeatable="true"/>
        <!-- Key android:codes="46,44" android:keyLabel=". ," android:keyWidth="12.5%p" /-->
        <Key android:codes="46" android:keyLabel="." android:keyWidth="12.5%p" />
        <Key android:codes="-9" android:keyIcon="@drawable/sym_keyboard_encrypt"
                android:keyWidth="16%p"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="17.5%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>