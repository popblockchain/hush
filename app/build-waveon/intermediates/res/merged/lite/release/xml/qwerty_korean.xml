<!-- Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    -->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="-8"  android:keyWidth="40%p" android:keyLabel="▦  QWERTY" android:keyEdgeFlags="left"
            android:keyHeight="@dimen/key_height_special"/>
        <Key android:codes="-13"  android:keyWidth="20%p" android:keyIcon="@drawable/sym_settings"
            android:keyHeight="@dimen/key_height_special"/>
        <!-- Key android:codes="-10"  android:keyWidth="30%p" android:keyLabel="그룹아이디"
            android:keyHeight="@dimen/key_height_special"
            / -->
        <Key android:codes="-9"  android:keyWidth="40%p" android:keyLabel="암호화/환원" android:keyEdgeFlags="right"
            android:keyHeight="@dimen/key_height_special"/>
    </Row>
    <Row>
        <Key android:codes="301" android:keyIcon="@drawable/sym_keyboard_b" android:keyEdgeFlags="left"/>
        <Key android:codes="302" android:keyIcon="@drawable/sym_keyboard_j"/>
        <Key android:codes="303" android:keyIcon="@drawable/sym_keyboard_d"/>
        <Key android:codes="304" android:keyIcon="@drawable/sym_keyboard_g"/>
        <Key android:codes="305" android:keyIcon="@drawable/sym_keyboard_s"/>
        <Key android:codes="306" android:keyIcon="@drawable/sym_keyboard_yo"/>
        <Key android:codes="307" android:keyIcon="@drawable/sym_keyboard_yeo"/>
        <Key android:codes="308" android:keyIcon="@drawable/sym_keyboard_ya"/>
        <Key android:codes="309" android:keyIcon="@drawable/sym_keyboard_ae"/>
        <Key android:codes="310" android:keyIcon="@drawable/sym_keyboard_e" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <!-- Key android:codes="-8" android:keyIcon="@drawable/sym_keyboard_hanhan" android:keyEdgeFlags="left"/ -->
        <Key android:horizontalGap="5%p" android:codes="311" android:keyIcon="@drawable/sym_keyboard_m" />
        <Key android:codes="312" android:keyIcon="@drawable/sym_keyboard_n"/>
        <Key android:codes="313" android:keyIcon="@drawable/sym_keyboard_ng"/>
        <Key android:codes="314" android:keyIcon="@drawable/sym_keyboard_r"/>
        <Key android:codes="315" android:keyIcon="@drawable/sym_keyboard_h"/>
        <Key android:codes="316" android:keyIcon="@drawable/sym_keyboard_o"/>
        <Key android:codes="317" android:keyIcon="@drawable/sym_keyboard_eo"/>
        <Key android:codes="318" android:keyIcon="@drawable/sym_keyboard_a"/>
        <Key android:codes="201" android:keyIcon="@drawable/sym_keyboard_i" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
                android:keyWidth="15%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="320" android:keyIcon="@drawable/sym_keyboard_k"/>
        <Key android:codes="321" android:keyIcon="@drawable/sym_keyboard_t"/>
        <Key android:codes="322" android:keyIcon="@drawable/sym_keyboard_ch"/>
        <Key android:codes="323" android:keyIcon="@drawable/sym_keyboard_p"/>
        <Key android:codes="324" android:keyIcon="@drawable/sym_keyboard_yu"/>
        <Key android:codes="325" android:keyIcon="@drawable/sym_keyboard_wu"/>
        <Key android:codes="203" android:keyIcon="@drawable/sym_keyboard_eu"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" 
                android:keyWidth="15%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-7" android:keyIcon="@drawable/sym_keyboard_numspec" 
                android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyIcon="@drawable/sym_keyboard_haneng" android:keyWidth="15%p"/>
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="37.5%p" android:isRepeatable="true"/>
        <Key android:codes="46" android:keyLabel="." android:keyWidth="12.5%p" />
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="20%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    